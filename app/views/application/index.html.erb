<h1><%= labelize(@model).pluralize %> <small> <%= link_to "New", action: :new %> </small></h1>
<%= back_button %>
<table>
  <thead>
    <tr>
      <th>id</th> <!-- TODO HACK -->
      <% @field_order.each do |key| %>
       <th><%= key %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @instances.each do |instance| %>
      <tr onclick=<%= show_instance instance %> >
         <th><%= instance.id %></th>
         <% @field_order.each do |key| %>
         <td><%= instance[key] %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>



